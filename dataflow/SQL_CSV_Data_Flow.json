{
	"name": "SQL_CSV_Data_Flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "lab_demo_storage",
						"type": "DatasetReference"
					},
					"name": "myFilesTestCSV",
					"description": "Import data from lab_demo_files (computer files)"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SqlDemoTable",
						"type": "DatasetReference"
					},
					"name": "sqlDemoSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          specnum_formatted as string,",
				"          specimen_id as string,",
				"          spec_numwheel as string,",
				"          spec_numyear as short,",
				"          spec_numnum as short,",
				"          gross_path_i1 as string,",
				"          gross_path_ia as string,",
				"          frozen_path_i1 as string,",
				"          frozen_path_ia as string,",
				"          case_path_i1 as string,",
				"          case_path_ia as string,",
				"          histo_event_when as string,",
				"          histo_event_name as string,",
				"          histo_event_ini as string,",
				"          priority_name as string,",
				"          priority_abbr as string,",
				"          part_designator as string,",
				"          part_description as string,",
				"          parttype_id as string,",
				"          part_inst as short,",
				"          part_sequence as short,",
				"          part_dt_taken as string,",
				"          part_dt_received as string,",
				"          part_label_design as string,",
				"          part_all_used as boolean,",
				"          part_addl_labels as string,",
				"          block_label as string,",
				"          block_order_date as string,",
				"          block_status_date as string,",
				"          block_pieces as short,",
				"          block_log_id as string,",
				"          block_log_name as string,",
				"          block_log_abbr as string,",
				"          block_req_cls_name as string,",
				"          block_req_cls_abbr as string,",
				"          block_dept_name as string,",
				"          block_dept_abbr as string,",
				"          block_comment as string,",
				"          block_sequence as short,",
				"          block_inst as short,",
				"          stain_order_date as string,",
				"          stain_status_date as string,",
				"          stain_sequence as short,",
				"          stain_slidecount as short,",
				"          stain_comment as string,",
				"          stain_id as string,",
				"          stain_label as string,",
				"          stain_ord_by as string,",
				"          stain_process_name as string,",
				"          stain_process_abbr as string,",
				"          stain_req_cls_name as string,",
				"          stain_req_cls_abbr as string,",
				"          stain_dept_name as string,",
				"          stain_dept_abbr as string,",
				"          stain_log_name as string,",
				"          stain_log_abbr as string,",
				"          stain_inst as short,",
				"          custom1 as string,",
				"          block_status_name as string,",
				"          block_status_abbr as string,",
				"          stain_status_name as string,",
				"          stain_status_abbr as string,",
				"          part_type_name as string,",
				"          accession_date as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> myFilesTestCSV",
				"myFilesTestCSV sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sqlDemoSink"
			]
		}
	}
}